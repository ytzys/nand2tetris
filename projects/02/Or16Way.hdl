/**
 * 16-bit incrementer:
 * out = in + 1 (arithmetic addition)
 */

CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
   // Put you code here:
	Or8Way(in=in[0..7], out=out1);
	Or8Way(in=in[8..15], out=out2);
	Or(a=out1, b=out2, out=out);
}
