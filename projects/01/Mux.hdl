/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in=sel, out=notsel);
	And(a=a, b=notsel, out=outand1);
	And(a=b, b=sel, out=outand2);
	Or(a=outand1, b=outand2, out=out);
}